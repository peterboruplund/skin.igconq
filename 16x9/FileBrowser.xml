<?xml version="1.0" encoding="UTF-8"?>
<window>
	<!-- filebrowser -->
	<defaultcontrol always="true">450</defaultcontrol>
	<include>WindowFadeAnimation</include>

	<controls>

		<!-- Background -->
		<include>WindowBackgroundImage</include>

		<!-- Curve -->
		<control type="image">
			<right>20</right>
			<top>20</top>
			<width>1280</width>
			<height>1040</height>
			<texture border="40,0,0,0">common/Curve.png</texture>
			<visible>Skin.HasSetting(ShowCurve)</visible>
		</control>

		<include content="Time">
			<param name="heading">$INFO[Control.GetLabel(411)]</param>
		</include>

		<!-- Heading -->
		<control type="label" id="411">
			<left>-15000</left>
			<top>96</top>
			<width>1620</width>
			<height>72</height>
			<font>Font72</font>
		</control>

		<!-- Image -->
		<control type="image">
			<left>150</left>
			<top>225</top>
			<width>405</width>
			<height>600</height>
			<texture>$INFO[ListItem.Icon]</texture>
			<aspectratio aligny="center">keep</aspectratio>
			<visible>!String.Contains(ListItem.Icon,Default) + !String.Contains(Control.GetLabel(416),*)</visible>
		</control>
		<control type="image">
			<left>150</left>
			<top>225</top>
			<width>405</width>
			<height>600</height>
			<texture flipx="true">$INFO[ListItem.Icon]</texture>
			<aspectratio aligny="center">keep</aspectratio>
			<visible>!String.Contains(ListItem.Icon,Default) + String.Contains(Control.GetLabel(416),*)</visible>
		</control>
		<control type="image">
			<left>150</left>
			<top>225</top>
			<width>405</width>
			<height>600</height>
			<texture>$INFO[ListItem.Icon]</texture>
			<colordiffuse>OverlayColorFO</colordiffuse>
			<aspectratio aligny="center">keep</aspectratio>
			<visible>String.Contains(ListItem.Icon,Default)</visible>
		</control>
		<control type="image">
			<left>150</left>
			<top>225</top>
			<width>405</width>
			<height>600</height>
			<texture>$INFO[ListItem.FileNameAndPath]</texture>
			<colordiffuse>OverlayColorFO</colordiffuse>
			<aspectratio aligny="center">keep</aspectratio>
			<visible>String.IsEqual(ListItem.Path,special://skin/extras/backgrounds/)</visible>
		</control>

		<!-- Scrollbar -->
		<control type="scrollbar" id="60">
			<left>690</left>
			<centertop>50%</centertop>
			<width>20</width>
			<height>720</height>
			<onleft>9000</onleft>
			<onright>450</onright>
			<showonepage>false</showonepage>
			<orientation>vertical</orientation>
			<colordiffuse>OverlayColorNF</colordiffuse>
			<texturesliderbackground border="11,1,1,1">common/ScrollBackground.png</texturesliderbackground>
			<texturesliderbar border="11,1,1,1">common/ScrollbarGripNF.png</texturesliderbar>
			<texturesliderbarfocus border="11,1,1,1" colordiffuse="$VAR[DialogColor1]">common/ScrollbarGripFO.png</texturesliderbarfocus>
			<textureslidernib>-</textureslidernib>
			<textureslidernibfocus>-</textureslidernibfocus>
			<visible>!Skin.HasSetting(Scrollbars)</visible>
		</control>

		<!-- List -->
		<control type="list" id="450">
			<left>750</left>
			<top>235</top>
			<width>1050</width>
			<height>672</height>
			<onup>noop</onup>
			<ondown>9000</ondown>
			<onleft>60</onleft>
			<onright>9000</onright>
			<orientation>vertical</orientation>
			<viewtype label="list">list</viewtype>
			<scrolltime tween="sine" easing="out">240</scrolltime>
			<pagecontrol>60</pagecontrol>

			<itemlayout width="1050" height="96">
				<control type="image">
					<width>96</width>
					<height>96</height>
					<bordersize>6</bordersize>
					<aspectratio>scale</aspectratio>
					<texture>$INFO[ListItem.Icon]</texture>
					<animation effect="fade" start="100" end="75" condition="True">Conditional</animation>
					<visible>!String.Contains(ListItem.Icon,Default)</visible>
				</control>
				<control type="image">
					<width>96</width>
					<height>96</height>
					<bordersize>6</bordersize>
					<aspectratio>scale</aspectratio>
					<colordiffuse>OverlayColorNF</colordiffuse>
					<texture>$INFO[ListItem.Icon]</texture>
					<animation effect="fade" start="100" end="75" condition="True">Conditional</animation>
					<visible>String.Contains(ListItem.Icon,Default)</visible>
				</control>
				<control type="label">
					<left>140</left>
					<width>910</width>
					<height>96</height>
					<font>Font36</font>
					<textcolor>$VAR[DialogColor2]</textcolor>
					<label>$INFO[ListItem.Label]</label>
				</control>
			</itemlayout>

			<focusedlayout width="1050" height="96">
				<control type="image">
					<width>96</width>
					<height>96</height>
					<bordersize>6</bordersize>
					<aspectratio>scale</aspectratio>
					<texture>$INFO[ListItem.Icon]</texture>
					<animation effect="fade" start="100" end="75" condition="!Control.HasFocus(450)">Conditional</animation>
					<visible>!String.Contains(ListItem.Icon,Default)</visible>
				</control>
				<control type="image">
					<width>96</width>
					<height>96</height>
					<bordersize>6</bordersize>
					<aspectratio>scale</aspectratio>
					<colordiffuse>OverlayColorFO</colordiffuse>
					<texture>$INFO[ListItem.Icon]</texture>
					<animation effect="fade" start="100" end="75" condition="!Control.HasFocus(450)">Conditional</animation>
					<visible>String.Contains(ListItem.Icon,Default)</visible>
				</control>
				<control type="label">
					<left>140</left>
					<width>910</width>
					<height>96</height>
					<font>Font36</font>
					<textcolor>$VAR[DialogColor1]</textcolor>
					<label>$INFO[ListItem.Label]</label>
					<visible>Control.HasFocus(450)</visible>
				</control>
				<control type="label">
					<left>140</left>
					<width>910</width>
					<height>96</height>
					<font>Font36</font>
					<textcolor>$VAR[DialogColor2]</textcolor>
					<label>$INFO[ListItem.Label]</label>
					<visible>!Control.HasFocus(450)</visible>
				</control>
			</focusedlayout>

		</control>

		<!-- List -->

		<control type="list" id="451">
			<left>750</left>
			<top>235</top>
			<width>1050</width>
			<height>672</height>
			<onup>noop</onup>
			<ondown>9000</ondown>
			<onleft>9000</onleft>
			<onright>9000</onright>
			<orientation>vertical</orientation>
			<scrolltime tween="sine" easing="out">240</scrolltime>

			<itemlayout width="1170" height="96">
				<control type="image">
					<width>96</width>
					<height>96</height>
					<bordersize>6</bordersize>
					<aspectratio>scale</aspectratio>
					<texture>$INFO[ListItem.Icon]</texture>
					<animation effect="fade" start="100" end="75" condition="True">Conditional</animation>
					<visible>!String.Contains(ListItem.Icon,Default)</visible>
				</control>
				<control type="image">
					<width>96</width>
					<height>96</height>
					<bordersize>6</bordersize>
					<aspectratio>scale</aspectratio>
					<colordiffuse>OverlayColorNF</colordiffuse>
					<texture>$INFO[ListItem.Icon]</texture>
					<animation effect="fade" start="100" end="75" condition="True">Conditional</animation>
					<visible>String.Contains(ListItem.Icon,Default)</visible>
				</control>
				<control type="label">
					<left>140</left>
					<width>910</width>
					<height>96</height>
					<font>Font36</font>
					<textcolor>$VAR[DialogColor2]</textcolor>
					<label>$INFO[ListItem.Label]</label>
				</control>
			</itemlayout>

			<focusedlayout width="1170" height="96">
				<control type="image">
					<width>96</width>
					<height>96</height>
					<bordersize>6</bordersize>
					<aspectratio>scale</aspectratio>
					<texture>$INFO[ListItem.Icon]</texture>
					<animation effect="fade" start="100" end="75" condition="!Control.HasFocus(451)">Conditional</animation>
					<visible>!String.Contains(ListItem.Icon,Default)</visible>
				</control>
				<control type="image">
					<width>96</width>
					<height>96</height>
					<bordersize>6</bordersize>
					<aspectratio>scale</aspectratio>
					<colordiffuse>OverlayColorFO</colordiffuse>
					<texture>$INFO[ListItem.Icon]</texture>
					<animation effect="fade" start="100" end="75" condition="!Control.HasFocus(451)">Conditional</animation>
					<visible>String.Contains(ListItem.Icon,Default)</visible>
				</control>
				<control type="label">
					<left>140</left>
					<width>910</width>
					<height>96</height>
					<font>Font36</font>
					<textcolor>$VAR[DialogColor1]</textcolor>
					<label>$INFO[ListItem.Label]</label>
					<visible>Control.HasFocus(451)</visible>
				</control>
				<control type="label">
					<left>140</left>
					<width>910</width>
					<height>96</height>
					<font>Font36</font>
					<textcolor>$VAR[DialogColor1]</textcolor>
					<label>$INFO[ListItem.Label]</label>
					<visible>!Control.HasFocus(451)</visible>
				</control>
			</focusedlayout>

		</control>

		<!-- Button grouplist -->
		<control type="grouplist" id="9000">
			<left>750</left>
			<top>912</top>
			<width>1620</width>
			<height>66</height>
			<itemgap>0</itemgap>
			<onleft>noop</onleft>
			<onright>noop</onright>
			<onup>450</onup>
			<ondown>noop</ondown>
			<orientation>horizontal</orientation>

			<!-- OK Button -->
			<control type="button" id="413">
				<width>324</width>
				<height>66</height>
				<label>186</label>
			</control>

			<!-- Cancel Button -->
			<control type="button" id="414">
				<width>324</width>
				<height>66</height>
				<label>222</label>
			</control>

			<!-- New folder -->
			<control type="button" id="415">
				<width>324</width>
				<height>66</height>
				<label>119</label>
				<visible>Control.IsEnabled(415)</visible>
			</control>

			<!-- Mirror image -->
			<control type="radiobutton" id="416">
				<width>324</width>
				<height>66</height>
				<label>749</label>
				<visible>Control.IsEnabled(416)</visible>
			</control>

		</control>

	</controls>

</window>